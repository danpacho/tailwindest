{
    "name": "tailwindest",
    "version": "3.1.0",
    "description": "typesafe, reusable tailwind",
    "homepage": "https://tailwindest.vercel.app",
    "author": "danpacho",
    "license": "MIT",
    "keywords": [
        "near zero bundle size",
        "type safe tailwind",
        "typed tailwind",
        "CSS in TS"
    ],
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "tsup src/index.ts --format=cjs,esm --dts",
        "build:fast": "tsup src/index.ts --format=cjs,esm"
    },
    "dependencies": {
        "clsx": "^2.1.1"
    },
    "devDependencies": {
        "tailwind-merge": "latest"
    }
}
